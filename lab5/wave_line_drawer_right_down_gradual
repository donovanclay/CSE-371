onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /line_drawer_right_down_gradual_tb/clk
add wave -noupdate /line_drawer_right_down_gradual_tb/reset
add wave -noupdate /line_drawer_right_down_gradual_tb/done
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/x0
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/y0
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/x1
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/y1
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/x
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/y
add wave -noupdate -radix unsigned /line_drawer_right_down_gradual_tb/i
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {150 ps} 0}
quietly wave cursor active 1
configure wave -namecolwidth 150
configure wave -valuecolwidth 100
configure wave -justifyvalue left
configure wave -signalnamewidth 1
configure wave -snapdistance 10
